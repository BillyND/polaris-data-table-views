{
  "name": "polaris-query",
  "version": "1.0.0",
  "description": "A client-side query library for Shopify Polaris that integrates with mongoose-url-query",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "polaris",
    "shopify",
    "query",
    "mongoose",
    "react",
    "hooks"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@shopify/polaris": ">=12.0.0",
    "react": ">=18.0.0",
    "react-router": ">=7.0.0"
  },
  "dependencies": {
    "mongoose-url-query": "^1.6.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@shopify/polaris": "^13.9.0",
    "@types/lodash": "^4.17.13",
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "react-router": "^7.1.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}

